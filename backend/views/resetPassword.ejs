      <div class="row d-flex justify-content-center align-items-center h-100">
        <div class="col-12 col-md-8 col-lg-6 col-xl-5">
          <div class="card bg-dark text-white" style="border-radius: 1rem;">
            <div class="card-body p-5 text-center">

                <h2 class="fw-bold mb-4 text-uppercase">Reset Password</h2>


<!-- action= "" method="PATCH" -->
                <form class="form">
                  <div class="form-outline form-white mb-4">
                    <label class="form-label" for="typeEmailX">New Password</label>
                    <input type="password" id="new" class="form-control form-control-lg" autocomplete='on' required />
                  </div>

                  <div class="form-outline form-white mb-4">
                    <label class="form-label" for="typePasswordX"> Confirm Password</label>
                    <input type="password" id="confirm" autocomplete='on' class="form-control form-control-lg"
                      required />

                    <div class="alart "></div>
                  </div>


                  <button class="btn btn-outline-light btn-lg px-5" type="submit">Submit</button>

                </form>
            </div>

          </div>
        </div>
      </div>
    </div>
    </div>
